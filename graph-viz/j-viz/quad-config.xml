<?xml version="1.0" encoding="UTF-8"?>

<timeseries grid_x="2" grid_y="2">
    <time range_from="0" range_to="60">Time</time>
    
    <plot pos_x="0" pos_y="0" range_from="0" range_to="5" title="Altitude" ylabel="Alt [ft]">
        <data>/fdm/jsbsim/position/h-agl-ft</data>
    </plot>

    <plot pos_x="1" pos_y="0" range_from="0" range_to="1" title="Throttle" ylabel="Thr [norm]">
        <data>/fdm/jsbsim/fcs/throttle-cmd-norm</data>
        <data>/fdm/jsbsim/fcs/throttle-cmd-norm[1]</data>
        <data>/fdm/jsbsim/fcs/throttle-cmd-norm[2]</data>
        <data>/fdm/jsbsim/fcs/throttle-cmd-norm[3]</data>
    </plot>


    <plot pos_x="0" pos_y="1" range_from="-0.00005" range_to="0.00005" title="Geo position" ylabel="Pos [deg]">
        <data>/fdm/jsbsim/position/long-gc-deg</data>
        <data>/fdm/jsbsim/position/lat-gc-deg</data>
    </plot>
</timeseries>