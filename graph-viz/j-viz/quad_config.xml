<?xml version="1.0" encoding="UTF-8"?>

<timeseries grid_x="5" grid_y="2">
    <time range_from="0" range_to="60">simulation/sim-time-sec</time>
    
    <plot pos_x="0" pos_y="0" range_from="0" range_to="2" title="Altitude" ylabel="Alt [m]">
        <data>ext/altitude-m</data>
    </plot>

    <plot pos_x="1" pos_y="0" range_from="-0.00005" range_to="0.00005" title="Geo position" ylabel="Pos [deg]">
        <data>ext/longitude-deg</data>
        <data>ext/latitude-deg</data>
    </plot>

    <plot pos_x="2" pos_y="0" range_from="-30" range_to="30" title="JSBSim attitude" ylabel="Angle [deg]">
        <data>attitude/psi-deg</data>
        <data>attitude/theta-deg</data>
        <data>attitude/phi-deg</data>
    </plot>

    <plot pos_x="3" pos_y="0" range_from="0" range_to="1" title="Throttle" ylabel="Thr [norm]">
        <data>fcs/throttle-cmd-norm[0]</data>
        <data>fcs/throttle-cmd-norm[1]</data>
        <data>fcs/throttle-cmd-norm[2]</data>
        <data>fcs/throttle-cmd-norm[3]</data>
    </plot>

    <plot pos_x="4" pos_y="0" range_from="0" range_to="1" title="User control" ylabel="[norm]">
        <data>user-control/channel-1</data>
        <data>user-control/channel-2</data>
        <data>user-control/channel-3</data>
        <data>user-control/channel-4</data>
    </plot>

    <plot pos_x="0" pos_y="1" range_from="-2" range_to="2" title="Acceleration" ylabel="[g]">
        <data>sensor/imu/accelX-g</data>
        <data>sensor/imu/accelY-g</data>
        <data>sensor/imu/accelZ-g</data>
    </plot>

    <plot pos_x="1" pos_y="1" range_from="-3" range_to="3" title="Rotation" ylabel="[rps]">
        <data>sensor/imu/gyroX-rps</data>
        <data>sensor/imu/gyroY-rps</data>
        <data>sensor/imu/gyroZ-rps</data>
    </plot>

    <plot pos_x="2" pos_y="1" range_from="-40" range_to="40" title="Magnetic flux" ylabel="[uT]">
        <data>sensor/imu/magX-uT</data>
        <data>sensor/imu/magY-uT</data>
        <data>sensor/imu/magZ-uT</data>
    </plot>

    <plot pos_x="3" pos_y="1" range_from="-30" range_to="30" title="Estimated attitude" ylabel="Angle [deg]">
        <data>fcs/psi-est</data>
        <data>fcs/theta-est</data>
        <data>fcs/phi-est</data>
    </plot>
</timeseries>
