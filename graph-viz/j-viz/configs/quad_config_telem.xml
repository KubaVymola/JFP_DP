<?xml version="1.0" encoding="UTF-8"?>

<timeseries grid_x="5" grid_y="2">
    <time range_from="0" range_to="60000">telem/simulation/sim-time-sec</time>
    
    <plot pos_x="0" pos_y="0" range_from="-2" range_to="2" title="Acceleration" ylabel="[mg]">
        <data>telem/sensor/imu/accelX-mg</data>
        <data>telem/sensor/imu/accelY-mg</data>
        <data>telem/sensor/imu/accelZ-mg</data>
    </plot>

    <plot pos_x="1" pos_y="0" range_from="-90" range_to="90" title="Attitude" ylabel="[deg]">
        <data>telem/yaw</data>
        <data>telem/pitch</data>
        <data>telem/roll</data>
    </plot>

    <plot pos_x="2" pos_y="0" range_from="0" range_to="1" title="Control" ylabel="[norm]">
        <data>telem/channel-0</data>
        <data>telem/channel-1</data>
        <data>telem/channel-2</data>
        <data>telem/channel-3</data>
    </plot>

    <plot pos_x="3" pos_y="0" range_from="240" range_to="260" title="Alt" ylabel="[m agl]">
        <data>telem/alt-est-m</data>
    </plot>

</timeseries>
