        <parameter name="pos_pid_p">
            <min>  0.1 </min>
            <max>  8.0 </max>
            <step> 0.01 </step>
            <init> 4.35 </init>
        </parameter>
        
        <parameter name="pos_pid_i">
            <min>  0.0001 </min>
            <max>  1.0 </max>
            <step> 0.001 </step>
            <init> 0.1 </init>
        </parameter>
    
        <parameter name="pos_pid_d">
            <min>  0.1 </min>
            <max>  8.0 </max>
            <step> 0.01 </step>
            <init> 6.66 </init>
        </parameter>
    </pass>
</autotune>