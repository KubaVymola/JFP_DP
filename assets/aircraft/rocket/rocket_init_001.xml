<?xml version="1.0"?>
<initialize name="rocket_init">
    <ubody unit="M/SEC"> 0.0 </ubody>
    <latitude unit="DEG"> 0.0 </latitude>
    <longitude unit="DEG"> 0.0 </longitude>

    <!-- Aircraft YAW - X-axis -->
    <psi unit="DEG"> 0.0 </psi>

    <!-- Aircraft PITCH - Y-axis -->
    <theta unit="DEG"> 45.0 </theta>

    <!-- Aircraft ROLL - Z-axis -->
    <phi unit="DEG"> 0.0 </phi>

    <altitude unit="M"> 0.3 </altitude>

</initialize>
